{
  "name": "chronobank-smart-contracts",
  "version": "1.1.47",
  "description": "Chronobank Smart Contracts",
  "main": "index.js",
  "scripts": {
    "release": "standard-version",
    "install": "cp truffle-config.js truffle.js",
    "test": "npx truffle test",
    "compile": "npx truffle compile",
    "compile:all": "npx truffle compile --all",
    "testrpc": "npx ganache-cli -g 1 --gasLimit 6000000 --trace_request true --verbose | grep -Ev \"FilterSubprovider|eth_getFilterChanges\"",
    "testgas": "NODE_ENV=gasreportgen ./node_modules/truffle/build/cli.bundled.js test",
    "gasreport": "./scripts/gas-report/gasreport.cli.js"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true
        }
      ]
    ]
  },
  "standard-version": {
    "skip": {
      "bump": true
    }
  },
  "author": "Mike Fluff <mf@mikefluff.com> (http://mikefluff.com)",
  "license": "AGPL-3.0",
  "dependencies": {
    "bs58": "^4.0.1",
    "buffer": "^5.0.6",
    "browser-stdout": "1.3.0",
    "commander": "2.9.0",
    "diff": "3.2.0",
    "growl": "1.9.2",
    "json3": "3.3.2",
    "lodash.create": "3.1.1",
    "truffle-artifactor": "2.1.5",
    "has-flag": "^1.0.0",
    "q": "^1.5.0",
    "abi-decoder": "^1.0.9",
    "truffle-hdwallet-provider": "mikefluff/truffle-hdwallet-provider#7a2eccca1ba17c235a3ef258914ca74ea5f13014",
    "node-json2html": "1.1",
    "jsdom": "11.6",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "@digix/contest": "^0.2.6",
    "@digix/tempo": "^0.2.0",
    "truffle": "4.1.13",
    "ganache-cli": "6.1.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "ethereumjs-wallet": "0.6.0",
    "standard-version": "^4.0.0"
  }
}
